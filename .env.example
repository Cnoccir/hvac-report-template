# Environment Variables Example

# Google Maps API Key for the interactive map
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY

# Configuration file to use (default.json is used if not specified)
NEXT_PUBLIC_CONFIG_FILE=default

# Set to true to enable console logging of actions
NEXT_PUBLIC_DEBUG_MODE=false

# Analytics tracking ID if using analytics
NEXT_PUBLIC_ANALYTICS_ID=
